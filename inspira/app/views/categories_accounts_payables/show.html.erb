<div class="mx-auto md:w-2/3 w-full flex">
  <div class="mx-auto">
    <% if notice.present? %>
      <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
    <% end %>

    <%= render 'categories_accounts_payables/categories_accounts_payable', categories_accounts_payable: @categories_accounts_payable %>

    <%=link_to("Edit ", edit_admins_backoffice_categories_accounts_payable_path(@categories_accounts_payable))%>
    <%= link_to "Back to categories accounts payables", admins_backoffice_categories_accounts_payables_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <div class="inline-block ml-2">
      <%#button_to "Destroy this categories accounts payable", @categories_accounts_payable, method: :delete, class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 font-medium" %>
    </div>
  </div>
</div>
