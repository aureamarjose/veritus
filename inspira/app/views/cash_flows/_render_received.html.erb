<tr id="cash_flow<%= cash_flow.id %>" class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600">
  <td class="px-6 py-4">
    <% if cash_flow.receiveds.any? %>
      <% cash_flow.receiveds.each do |received| %>
        <%= I18n.l(received.lauch_date, format: :default) %>
      <% end %>
    <% end %>
  </td>
  <td class="px-6 py-4 text-center">
    <% if cash_flow.receiveds.any? %>
      <% cash_flow.receiveds.each do |received| %>
        <p>Pasta: <%= received.service.folder %> </p>
      <% end %>
    <% end %>
  </td>
  <td class="px-6 py-4 text-center">
     <% if cash_flow.receiveds.any? %>
      <% cash_flow.receiveds.each do |received| %>
        <p><%= received.service.client.name_client %> </p>
      <% end %>
    <% end %>
  </td>
  <td class="px-6 py-4 text-center">
     <% if cash_flow.receiveds.any? %>
      <% cash_flow.receiveds.each do |received| %>
        <% if received.service.address.present?%>
          <p><%= received.service.address.street %> </p>
        <%else%>
          <p>Não há endereço cadastrado</p>
        <%end%>
      <% end %>
    <% end %>
  </td>
  <td class="px-6 py-4 text-center">
    <%= number_to_currency(cash_flow.lauch_value, unit: "R$ ", separator: ",", delimiter: ".") %>
  </td>
  <td class="px-6 py-4 text-center">
    <% if cash_flow.lauch_type == "Entrada" %>
      <p class="text-sm font-semibold text-gray-900 dark:text-white"><%= number_to_currency(cash_flow.balance, unit: "R$ ", separator: ",", delimiter: ".") %></p>
    <% else %>
      <p class="text-sm font-semibold text-gray-900 dark:text-white"><%= number_to_currency(cash_flow.balance, unit: "R$ ", separator: ",", delimiter: ".") %></p>
    <% end %>
  </td>
</tr>