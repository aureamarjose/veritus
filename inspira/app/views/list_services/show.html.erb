<div class="mx-auto md:w-2/3 w-full flex">
  <div class="mx-auto">
    <% if notice.present? %>
      <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
    <% end %>

     <%= render partial: 'list_services/list_service', locals: { list_service: @list_service } %>
    <% if @logger %>
      <%= link_to "Edit this list service", edit_admins_backoffice_list_service_path(@list_service), class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%else%>
      <%= link_to "Edit this list service", edit_users_backoffice_list_service_path(@list_service), class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
    <%end%>

    <%= @logger == true ? link_to( "Back to list services", admins_backoffice_list_services_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium") : link_to( "Back to list services", users_backoffice_list_services_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium") %>
    <div class="inline-block ml-2">
      <%= @logger == true ? button_to( "Destroy this list service", [:admins_backoffice,@list_service], method: :delete, class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 font-medium") : button_to( "Destroy this list service", [:users_backoffice,@list_service], method: :delete, class: "mt-2 rounded-lg py-3 px-5 bg-gray-100 font-medium") %>
    </div>
  </div>
</div>


